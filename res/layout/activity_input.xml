<android.support.v4.widget.DrawerLayout
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:id="@+id/drawer_layout_input"
android:layout_width="match_parent"
android:layout_height="match_parent" >

<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/scrollView1"
	android:layout_width="match_parent"
	android:layout_height="match_parent" >

	<LinearLayout
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:orientation="vertical" >
	
	    <TextView
	        android:id="@+id/TextView_Off_Title"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginBottom="-10dp"
	        android:layout_marginLeft="10dp"
	        android:layout_marginTop="10dp"
	        android:background="@drawable/border"
	        android:text="@string/off"
	        android:textAppearance="?android:attr/textAppearanceSmall"
	        android:textColor="@color/title" />

	    <LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="20dp"
	        android:orientation="horizontal" >
	
	        <TextView
	            android:id="@+id/TextView_2pt_Label"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:text="@string/two" />
	
	        <EditText
	            android:id="@+id/ET_2pt"
	            android:nextFocusRight="@+id/ET_2pt_A"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:inputType="number"
	            android:maxLength="3"
	            android:gravity="center_horizontal"
	            android:ems="10" />
	
	        <TextView
	            android:id="@+id/TextView07"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="0.30"
	            android:gravity="center_horizontal"
	            android:text="/" />
	
	        <EditText
	            android:id="@+id/ET_2pt_A"
	            android:nextFocusLeft="@+id/ET_2pt"
	            android:nextFocusRight="@+id/ET_3pt"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:inputType="number"
	            android:maxLength="3"
	            android:gravity="center_horizontal"
	            android:ems="10" />
	    </LinearLayout>	    
	    
	    <LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="20dp"
	        android:orientation="horizontal" >
	
	        <TextView
	            android:id="@+id/TextView_3pt_Label"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:text="@string/three" />
	
	        <EditText
	            android:id="@+id/ET_3pt"
	            android:nextFocusRight="@+id/ET_3pt_A"
	            android:nextFocusLeft="@+id/ET_2pt_A"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:inputType="number"
	            android:maxLength="3"
	            android:ems="10"
	            android:gravity="center_horizontal">
	        </EditText>
	
	        <TextView
	            android:id="@+id/TextView05"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="0.30"
	            android:gravity="center_horizontal"
	            android:text="/" />
	
	        <EditText
	            android:id="@+id/ET_3pt_A"
	            android:nextFocusRight="@+id/ET_ft"
	            android:nextFocusLeft="@+id/EF_3pt"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:inputType="number"
	            android:maxLength="3"
	            android:gravity="center_horizontal"
	            android:ems="10" />
	    </LinearLayout>
	

	    <LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="20dp"
	        android:orientation="horizontal" >
	
	        <TextView
	            android:id="@+id/TextView_ft_Label"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:text="@string/FT" />
	
	        <EditText
	            android:id="@+id/ET_ft"
	            android:nextFocusLeft="@+id/ET_3pt_A"
	            android:nextFocusRight="@+id/ET_ft_A"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:inputType="number"
	            android:maxLength="3"
	            android:gravity="center_horizontal"
	            android:ems="10" />
	
	        <TextView
	            android:id="@+id/TextView07"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="0.30"
	            android:gravity="center_horizontal"
	            android:text="/" />
	
	        <EditText
	            android:id="@+id/ET_ft_A"
	            android:nextFocusRight="@+id/ET_ast"
	            android:nextFocusLeft="@+id/ET_ft"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:inputType="number"
	            android:maxLength="3"
	            android:gravity="center_horizontal"
	            android:ems="10" />
	    </LinearLayout>	    
	    	
	    <LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="20dp"
	        android:orientation="horizontal" >
	
	        <TextView
	            android:id="@+id/TextView_ast_Label"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:text="@string/ast" />
	
	        <EditText
	            android:id="@+id/ET_ast"
	            android:nextFocusLeft="@+id/ET_ft_A"
	            android:nextFocusRight="@+id/ET_oreb"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="2"
	            android:inputType="number"
	            android:maxLength="3"
	            android:gravity="center_horizontal"
	            android:ems="10" />
	
	    </LinearLayout>
	
	    <LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="20dp"
	        android:orientation="horizontal" >
	
	        <TextView
	            android:id="@+id/TextView_oreb_Label"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:text="@string/oreb" />
	
	        <EditText
	            android:id="@+id/ET_oreb"
	            android:nextFocusLeft="@+id/ET_ast"
	            android:nextFocusRight="@+id/ET_stl"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="2.00"
	            android:inputType="number"
	            android:maxLength="3"
	            android:gravity="center_horizontal"
	            android:ems="10" />
	
	    </LinearLayout>
	
	    <TextView
	        android:id="@+id/TextView_Def_Title"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginBottom="0dp"
	        android:layout_marginLeft="10dp"
	        android:layout_marginTop="10dp"
	        android:background="@drawable/border"
	        android:text="@string/def"
	        android:textAppearance="?android:attr/textAppearanceSmall"
	        android:textColor="@color/title" />
	
	    <LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="20dp"
	        android:orientation="horizontal" >
	
	        <TextView
	            android:id="@+id/TextView_stl_Label"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:text="@string/stl" />
	
	        <EditText
	            android:id="@+id/ET_stl"
	            android:nextFocusLeft="@+id/ET_oreb"
	            android:nextFocusRight="@+id/ET_blk"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="2.00"
	            android:inputType="number"
	            android:maxLength="3"
	            android:gravity="center_horizontal"
	            android:ems="10" />
	
	    </LinearLayout>
	
	    <LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="20dp"
	        android:orientation="horizontal" >
	
	        <TextView
	            android:id="@+id/TextView_blk_Label"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:text="@string/blk" />
	
	        <EditText
	            android:id="@+id/ET_blk"
	            android:nextFocusLeft="@+id/ET_stl"
	            android:nextFocusRight="@+id/ET_dreb"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="2.00"
	            android:inputType="number"
	            android:maxLength="3"
	            android:gravity="center_horizontal"
	            android:ems="10" />
	
	    </LinearLayout>
	
	    <LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="20dp"
	        android:orientation="horizontal" >
	
	        <TextView
	            android:id="@+id/TextView_dreb_Label"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:text="@string/dreb" />
	
	        <EditText
	            android:id="@+id/ET_dreb"
	            android:nextFocusLeft="@+id/ET_blk"
	            android:nextFocusRight="@+id/ET_to"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="2.00"
	            android:inputType="number"
	            android:maxLength="3"
	            android:gravity="center_horizontal"
	            android:ems="10" />
	
	    </LinearLayout>
	
	    <TextView
	        android:id="@+id/TextView_Ext_Title"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginBottom="-5dp"
	        android:layout_marginLeft="10dp"
	        android:layout_marginTop="10dp"
	        android:background="@drawable/border"
	        android:text="@string/ext"
	        android:textAppearance="?android:attr/textAppearanceSmall"
	        android:textColor="@color/title" />
	
	    <LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="20dp"
	        android:orientation="horizontal" >
	
	        <TextView
	            android:id="@+id/TextView_to_Label"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:text="@string/to" />
	
	        <EditText
	            android:id="@+id/ET_to"
	            android:nextFocusLeft="@+id/ET_dreb"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="2.00"
	            android:inputType="number"
	            android:maxLength="3"
	            android:gravity="center_horizontal"
	            android:ems="10" />
	
	    </LinearLayout>
	
	    <Button
	        android:id="@+id/button1"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_margin="15dp"
	        android:background="@drawable/button_default"
	        android:textColor="#E6E8E7"
	        android:text="記録" />
	</LinearLayout>
</ScrollView>
   
<ListView
  android:id="@+id/drawer_listview_input"
  android:layout_width="300dp"
    android:layout_height="match_parent"
    android:layout_gravity="start"
    android:choiceMode="singleChoice"
    android:divider="@color/navi_divide"
    android:dividerHeight="2dp"
    android:background="@color/navi_background" />
</android.support.v4.widget.DrawerLayout>
