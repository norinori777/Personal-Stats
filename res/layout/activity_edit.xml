<android.support.v4.widget.DrawerLayout
xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:id="@+id/drawer_layout_input"
android:layout_width="match_parent"
android:layout_height="match_parent" >

<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ScrollView2"
    android:layout_width="match_parent"
    android:layout_height="wrap_content" >    
	<LinearLayout
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:orientation="vertical" >
	
	    <TextView
	        android:id="@+id/textView1"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginBottom="-10dp"
	        android:layout_marginLeft="10dp"
	        android:layout_marginTop="10dp"
	        android:background="@drawable/border"
	        android:text="@string/off"
	        android:textAppearance="?android:attr/textAppearanceSmall"
	        android:textColor="@color/title" />
	
	    <LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="20dp"
	        android:orientation="horizontal" >
	
	        <TextView
	            android:id="@+id/Label_two"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:text="@string/two" />
	
	        <EditText
	            android:id="@+id/EditText_2pt"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:nextFocusRight="@+id/EditText_2pt_attempt"
	            android:inputType="number"
	            android:gravity="center_horizontal"
	            android:ems="10" />
	
	        <TextView
	            android:id="@+id/TextView07"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="0.30"
	            android:gravity="center_horizontal"
	            android:text="/" />
	
	        <EditText
	            android:id="@+id/EditText_2pt_attempt"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:nextFocusLeft="@+id/EditText_2pt"
	            android:nextFocusRight="@+id/EditText_3pt"
	            android:inputType="number"
	            android:gravity="center_horizontal"
	            android:ems="10" />
	    </LinearLayout>

	    <LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="20dp"
	        android:orientation="horizontal" >
	
	        <TextView
	            android:id="@+id/Label_three"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:text="@string/three" />
	
	        <EditText
	            android:id="@+id/EditText_3pt"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:nextFocusLeft="@+id/EditText_2pt_attempt"
	            android:nextFocusRight="@+id/EditText_3pt_attempt"
	            android:inputType="number"
	            android:ems="10"
	            android:gravity="center_horizontal">
	        </EditText>
	
	        <TextView
	            android:id="@+id/TextView05"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="0.30"
	            android:gravity="center_horizontal"
	            android:text="/" />
	
	        <EditText
	            android:id="@+id/EditText_3pt_attempt"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:nextFocusLeft="@+id/EditText_3pt"
	            android:nextFocusRight="@+id/EditText_ft"
	            android:inputType="number"
	            android:gravity="center_horizontal"
	            android:ems="10" />
	    </LinearLayout>

	    <LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="20dp"
	        android:orientation="horizontal" >
	
	        <TextView
	            android:id="@+id/Label_ft"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:text="@string/FT" />
	
	        <EditText
	            android:id="@+id/EditText_ft"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:nextFocusLeft="@+id/EditText_3pt_attempt"
	            android:nextFocusRight="@+id/EditText_ft_attempt"
	            android:inputType="number"
	            android:gravity="center_horizontal"
	            android:ems="10" />
	
	        <TextView
	            android:id="@+id/TextView07"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="0.30"
	            android:gravity="center_horizontal"
	            android:text="/" />
	
	        <EditText
	            android:id="@+id/EditText_ft_attempt"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:nextFocusLeft="@+id/EditText_ft"
	            android:nextFocusRight="@+id/EditText_ast"
	            android:inputType="number"
	            android:gravity="center_horizontal"
	            android:ems="10" />
	    </LinearLayout>
	    	
	    <LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="20dp"
	        android:orientation="horizontal" >
	
	        <TextView
	            android:id="@+id/label_ast"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:text="@string/ast" />
	
	        <EditText
	            android:id="@+id/EditText_ast"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="2"
	            android:nextFocusLeft="@+id/EditText_ft_attemptt"
	            android:nextFocusRight="@+id/EditText_oreb"
	            android:inputType="number"
	            android:gravity="center_horizontal"
	            android:ems="10" />
	
	    </LinearLayout>
	
	    <LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="20dp"
	        android:orientation="horizontal" >
	
	        <TextView
	            android:id="@+id/label_oreb"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:text="@string/oreb" />
	
	        <EditText
	            android:id="@+id/EditText_oreb"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="2.00"
	            android:nextFocusLeft="@+id/EditText_ast"
	            android:nextFocusRight="@+id/EditText_stl"
	            android:inputType="number"
	            android:gravity="center_horizontal"
	            android:ems="10" />
	
	    </LinearLayout>
	
	    <TextView
	        android:id="@+id/TextView2"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginBottom="0dp"
	        android:layout_marginLeft="10dp"
	        android:layout_marginTop="10dp"
	        android:background="@drawable/border"
	        android:text="@string/def"
	        android:textAppearance="?android:attr/textAppearanceSmall"
	        android:textColor="@color/title" />
	
	    <LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="20dp"
	        android:orientation="horizontal" >
	
	        <TextView
	            android:id="@+id/label_stl"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:text="@string/stl" />
	
	        <EditText
	            android:id="@+id/EditText_stl"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="2.00"
	            android:nextFocusLeft="@+id/EditText_ast"
	            android:nextFocusRight="@+id/EditText_blk"
	            android:inputType="number"
	            android:gravity="center_horizontal"
	            android:ems="10" />
	
	    </LinearLayout>
	
	    <LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="20dp"
	        android:orientation="horizontal" >
	
	        <TextView
	            android:id="@+id/label_blk"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:text="@string/blk" />
	
	        <EditText
	            android:id="@+id/EditText_blk"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="2.00"
	            android:inputType="number"
	            android:nextFocusLeft="@+id/EditText_stl"
	            android:nextFocusRight="@+id/EditText_dreb"
	            android:gravity="center_horizontal"
	            android:ems="10" />
	
	    </LinearLayout>
	
	    <LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="20dp"
	        android:orientation="horizontal" >
	
	        <TextView
	            android:id="@+id/label_dreb"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:text="@string/dreb" />
	
	        <EditText
	            android:id="@+id/EditText_dreb"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="2.00"
	            android:inputType="number"
	            android:nextFocusLeft="@+id/EditText_blk"
	            android:nextFocusRight="@+id/EditText_to"
	            android:gravity="center_horizontal"
	            android:ems="10" />
	
	    </LinearLayout>
	
	    <TextView
	        android:id="@+id/TextView3"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginBottom="-5dp"
	        android:layout_marginLeft="10dp"
	        android:layout_marginTop="10dp"
	        android:background="@drawable/border"
	        android:text="@string/ext"
	        android:textAppearance="?android:attr/textAppearanceSmall"
	        android:textColor="@color/title" />
	
	    <LinearLayout
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="20dp"
	        android:orientation="horizontal" >
	
	        <TextView
	            android:id="@+id/label_to"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="1"
	            android:text="@string/to" />
	
	        <EditText
	            android:id="@+id/EditText_to"
	            android:layout_width="0dp"
	            android:layout_height="wrap_content"
	            android:layout_weight="2.00"
	            android:inputType="number"
	            android:nextFocusLeft="@+id/EditText_dreb"
	            android:gravity="center_horizontal"
	            android:ems="10" />
	
	    </LinearLayout>
	
	    <Button
	        android:id="@+id/edit_button"
	        android:layout_width="match_parent"
	        android:layout_height="wrap_content"
	        android:layout_margin="15dp"
	        android:background="@drawable/button_default"
	        android:textColor="#E6E8E7"
	        android:text="編集" />
	</LinearLayout>
</ScrollView>
<ListView
  android:id="@+id/drawer_listview_input"
  android:layout_width="300dp"
    android:layout_height="match_parent"
    android:layout_gravity="start"
    android:choiceMode="singleChoice"
    android:divider="@color/navi_divide"
    android:dividerHeight="2dp"
    android:background="@color/navi_background" />
</android.support.v4.widget.DrawerLayout>